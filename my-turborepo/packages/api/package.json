    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@vanni/prettier-config",
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.675.0",
    "@aws-sdk/credential-provider-node": "^3.669.0",
    "@trpc/server": "11.0.0-rc.364",
    "@vanni/auth": "workspace:*",
    "@vanni/db": "workspace:*",
    "@vanni/validators": "workspace:*",
    "superjson": "2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.664.0",
    "@vanni/eslint-config": "workspace:*",
    "@vanni/prettier-config": "workspace:*",
    "@vanni/tsconfig": "workspace:*",
    "eslint": "^9.2.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  }
