{
  "name": "@vanni/team-website",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev --port 3002",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "pnpm with-env next start --port 3002",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "next": "14.2.3",
    "next-themes": "^0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-modal-video": "^2.0.1"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "^20.12.9",
    "@types/react": "^18.3.2",
    "@vanni/eslint-config": "workspace:*",
    "@vanni/prettier-config": "workspace:*",
    "@vanni/tailwind-config": "workspace:*",
    "@vanni/tsconfig": "workspace:*",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.2.0",
    "postcss": "^8.4.41",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  }
}
